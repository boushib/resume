<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
    <servlet-name>CreateResume</servlet-name>
    <servlet-class>com.boushib.servlets.CreateResume</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreateResume</servlet-name>
    <url-pattern>/create-resume</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>My Resume</servlet-name>
    <servlet-class>com.boushib.servlets.MyResume</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>My Resume</servlet-name>
    <url-pattern>/my-resume</url-pattern>
  </servlet-mapping>
  
</web-app>
